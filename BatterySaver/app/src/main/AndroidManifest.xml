<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.niranjanpc.batterysaver">

    <uses-permission android:name="android.permission.BLUETOOTH" />//get access to bluetooth
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />//disable bluetooth
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />//get access to wifi
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>// system brihtness setting
    <uses-permission android:name="android.permission.INTERNET" />//data connection
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>//gps

    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />//kill process
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Help"></activity>


    </application>

</manifest>